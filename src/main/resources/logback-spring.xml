<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <include resource = "org/springframework/boot/logging/logback/base.xml" />

    <logger name = "org.springframework.boot" level="INFO"/>
    <logger name = "org.springframework.data.redis" level="debug"/>
<!--    <logger name = "org.springframework.cache" level="debug"/>-->
<!--    <logger name = "org.springframework.core" level="debug"/>-->
    <logger name = "org.springframework.web.reactive" level="info"/>
    <logger name = "org.reactivestreams" level="info"/>
    <logger name = "com.mytest.app" level="info"/>
    <logger name = "reactor.core.publisher" level="debug"/>
    <logger name = "reactor.netty.channel" level="info"/>
    <logger name = "io.netty.resolver." level="info"/>
<!--    <logger name = "io.lettuce.core" level="debug"/>-->

    <root level="INFO">
        <appender-ref ref="CONSOLE" />
    </root>
</configuration>